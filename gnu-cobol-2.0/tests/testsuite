cat >caller.c <<'_ATEOF'

#include <libcob.h>


int callee (char *, char *);

#ifndef NULL
#define NULL (void*)0
#endif

int
main (int argc, char **argv)
{
	cob_global *cobol_global;
	char *p1 = "A";

	cob_init(argc, argv);
	cobol_global = cob_get_global_ptr ();
	cobol_global->cob_call_params = 1;
	return callee (p1, NULL);
}
_ATEOF


