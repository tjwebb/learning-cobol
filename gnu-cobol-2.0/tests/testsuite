#AT_START_640
at_fn_group_banner 640 'run_extensions.at:3370' \
  "System routine CBL_OC_HOSTED" "                   " 4
at_xfail=no
(
  $as_echo "640. $at_setup_line: testing $at_desc ..."
  $at_traceon


cat >test_errno.c <<'_ATEOF'

#include <errno.h>
#include <stdio.h>

#define COB_LIB_EXPIMP
#include <libcob.h>
COB_EXPIMP int test_errno(void);

int
test_errno(void)
{
    FILE *fail;
    fail = fopen("file-not-found", "r");
    if (errno != 2) {
        printf("BAD ERRNO %d", errno);
    } else {
        if (fail) fclose(fail);
    }
    return 0;
}
_ATEOF


cat >test_stdio.c <<'_ATEOF'

#include <stdio.h>

#define COB_LIB_EXPIMP
#include <libcob.h>
COB_EXPIMP int test_stdio(FILE *, FILE *, FILE *);

int
test_stdio(FILE *si, FILE *so, FILE *se)
{
    if (feof(si)) {
        fprintf(se, "BAD STDIN EOF\n");
    }
    fprintf(so, "OUT");
    return fprintf(se, "ERR");
}
_ATEOF

cat >prog.cob <<'_ATEOF'

       IDENTIFICATION DIVISION.
       PROGRAM-ID. prog.
       DATA DIVISION.
       WORKING-STORAGE SECTION.
       01 STDIN    USAGE POINTER.
       01 STDOUT   USAGE POINTER.
       01 STDERR   USAGE POINTER.
       01 CELL     USAGE BINARY-LONG.
       01 ARGC     USAGE BINARY-LONG.
       01 ARGV     USAGE POINTER.
       01 ERRNO    USAGE POINTER.
       01 ERR      USAGE BINARY-LONG BASED.
       01 TZNAME   USAGE POINTER.
       01 TZNAMES  USAGE POINTER BASED.
          05 TZS      USAGE POINTER OCCURS 2 TIMES.
       01 TIMEZONE USAGE BINARY-C-LONG.
       01 DAYLIGHT USAGE BINARY-LONG.

       PROCEDURE DIVISION.
       CALL "CBL_OC_HOSTED" USING STDIN "stdin" END-CALL
       CALL "CBL_OC_HOSTED" USING STDOUT "stdout" END-CALL
       CALL "CBL_OC_HOSTED" USING STDERR "stderr" END-CALL
       CALL "CBL_OC_HOSTED" USING ARGC "argc" END-CALL
       CALL "CBL_OC_HOSTED" USING ARGV "argv" END-CALL
       CALL "CBL_OC_HOSTED" USING CELL "cell" END-CALL
       CALL "CBL_OC_HOSTED" USING ERRNO "errno" END-CALL
       CALL "CBL_OC_HOSTED" USING ARGC "arg" END-CALL
       CALL "CBL_OC_HOSTED" USING NULL "argc" END-CALL
       SET ADDRESS OF ERR TO ERRNO
       CALL "CBL_OC_HOSTED" USING TZNAME "tzname" END-CALL
       CALL "CBL_OC_HOSTED" USING TIMEZONE "timezone" END-CALL
       CALL "CBL_OC_HOSTED" USING DAYLIGHT "daylight" END-CALL

       CALL "test_errno"
       END-CALL
       IF ERR NOT EQUAL 2 THEN
           DISPLAY "BAD ERRNO " ERR END-DISPLAY
       END-IF

       IF ARGC NOT EQUAL 2 THEN
           DISPLAY "BAD ARGC " ARGC END-DISPLAY
       END-IF
       IF ARGV EQUAL NULL THEN
           DISPLAY "BAD ARGV" END-DISPLAY
       END-IF

       IF CELL LESS THAN 0 OR GREATER THAN 8 THEN
           DISPLAY "UNK CELL " CELL END-DISPLAY
       END-IF

       SET ENVIRONMENT "TZ" TO "PST8PDT"
       CALL "tzset" RETURNING OMITTED
           ON EXCEPTION CONTINUE
       END-CALL
       IF TZNAME NOT EQUAL NULL THEN
           SET ADDRESS OF TZNAMES TO TZNAME
           IF TZS(1) EQUAL NULL THEN
               DISPLAY "BAD TZNAME" END-DISPLAY
           END-IF
       END-IF
       IF DAYLIGHT NOT EQUAL 0 AND 1 THEN
           DISPLAY "BAD DAYLIGHT" DAYLIGHT END-DISPLAY
       END-IF

       *> Test assumes return-code will be 3, chars output by last fprintf
       CALL "test_stdio" USING BY VALUE STDIN STDOUT STDERR
       END-CALL

       STOP RUN.
_ATEOF


{ set +x
$as_echo "$at_srcdir/run_extensions.at:3483: \$COMPILE_MODULE test_errno.c"
at_fn_check_prepare_dynamic "$COMPILE_MODULE test_errno.c" "run_extensions.at:3483"
( $at_check_trace; $COMPILE_MODULE test_errno.c
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/run_extensions.at:3483"
$at_failed && at_fn_log_failure
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/run_extensions.at:3484: \$COMPILE_MODULE test_stdio.c"
at_fn_check_prepare_dynamic "$COMPILE_MODULE test_stdio.c" "run_extensions.at:3484"
( $at_check_trace; $COMPILE_MODULE test_stdio.c
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/run_extensions.at:3484"
$at_failed && at_fn_log_failure
$at_traceon; }

{ set +x
$as_echo "$at_srcdir/run_extensions.at:3485: \$COMPILE prog.cob"
